<odoo>
    <data>
        <record id="barcode_report_wizard_view" model="ir.ui.view">
            <field name="name">barcode.report.wizard.view</field>
            <field name="model">barcode.wizard.report</field>
            <field name="arch" type="xml">
                <form string="Barcode Wizard">
                    <sheet>
                        <group>
                            <field name="name"/>

                        </group>
                    </sheet>
                    <footer>
                        <button name="send_message" string="Send Message" type="object" class="btn-primary"/>
                        <button string="Cancel" class="btn-default" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>


    </data>
</odoo>